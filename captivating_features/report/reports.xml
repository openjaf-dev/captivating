        <field name="model">sale.order.line</field>
        <field name="report_name">travel_voucher_report</field>
        <field name="report_type">aeroo</field>
        <field name="in_format">oo-odt</field>
        <field name="out_format" model="report.mimetypes" search="[('code', '=', 'oo-pdf')]"/>
        <field name="report_rml">captivating_features/report/travel_voucher.odt</field>
        <field name="parser_state">default</field>
        <field name="tml_source">file</field>
    </record>

    <record id="action_binding1" model="ir.values">
        <field eval="'action'" name="key"/>
        <field eval="'client_print_multi'" name="key2"/>
        <field name="model">sale.order.line</field>
        <field name="name">Travel Voucher</field>
        <field eval="'ir.actions.report.xml,'+str(travel_voucher_report_id)" name="value"/>
    </record>

